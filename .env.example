# LLM Provider Configuration
MODEL=gpt-4.1-mini
MODEL_PROVIDER=openai

# API Keys
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
GOOGLE_API_KEY=your-google-api-key
XAI_API_KEY=your-xai-api-key

# Financial Data (Options: massive, financial_datasets)
FINANCIAL_DATA_PROVIDER=financial_datasets
FINANCIAL_DATASETS_API_KEY=your-financial-datasets-api-key
MASSIVE_API_KEY=your-massive-api-key

# Web Search (Optional)
TAVILY_API_KEY=your-tavily-api-key

# Ollama Configuration (for local models)
OLLAMA_BASE_URL=http://127.0.0.1:11434

# --- POLYMARKET REAL TRADING CONFIGURATION ---
# ONLY your Private Key is required. The bot automatically discovers your proxy
# and derives API credentials on-the-fly.

# Your Polygon wallet private key (starts with 0x...)
# It can be exported in Polymarket web-app
POLYMARKET_WALLET_PRIVATE_KEY=your-private-key-here

# Optional: Custom RPC URL (defaults to public endpoint if empty)
POLYGON_RPC_URL=https://polygon-rpc.com

# [ADVANCED] Optional Overrides (Leave commented for auto-setup)
# POLYMARKET_PROXY_ADDRESS=
# POLYMARKET_API_KEY=
# POLYMARKET_SECRET=
# POLYMARKET_PASSPHRASE=
# POLYMARKET_SIGNATURE_TYPE=1

# Weather APIs
TOMORROWIO_API_KEY=...
VISUAL_CROSSING_API_KEY=...

# Debug Mode
FINCODE_DEBUG=false